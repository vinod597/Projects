package com.pbsi.fuelfilling.bean;

import java.util.HashMap;
import java.util.Map;

public class GridBean {
	private Integer currentRecord;
	private Integer count;
	private Map<String, Integer> sortingAscOrDescMap = new HashMap<String, Integer>();
	private Map<String, Integer> sortedColumnMap = new HashMap<String, Integer>();

	public Integer getCurrentRecord() {
		return currentRecord;
	}

	public void setCurrentRecord(Integer currentRecord) {
		this.currentRecord = currentRecord;
	}

	public Integer getCount() {
		return count;
	}

	public void setCount(Integer count) {
		this.count = count;
	}

	public Map<String, Integer> getSortingAscOrDescMap() {
		return sortingAscOrDescMap;
	}

	public void setSortingAscOrDescMap(Map<String, Integer> sortingAscOrDescMap) {
		this.sortingAscOrDescMap = sortingAscOrDescMap;
	}

	public Map<String, Integer> getSortedColumnMap() {
		return sortedColumnMap;
	}

	public void setSortedColumnMap(Map<String, Integer> sortedColumnMap) {
		this.sortedColumnMap = sortedColumnMap;
	}

}
